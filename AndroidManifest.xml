        <activity android:name="com.example.esankalp.EduWeb" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.esankalp.HealthVideo" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.esankalp.HealthWeb" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.esankalp.IgVideo" android:screenOrientation="landscape"> </activity>
        <activity android:name="com.example.esankalp.IgWeb" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.esankalp.NsVideo" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.esankalp.NsWeb" android:screenOrientation="landscape"></activity>
    </application>

</manifest>
